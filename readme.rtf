\par \pard\plain \s22\ql\nowidctlpar\hyphpar0\sb0\sa0\ltrpar\cf1\dbch\af7\langfe1081\dbch\af9\afs24\loch\f5\fs20\lang1033\sb0\sa0\rtlch \ltrch\loch

